    <script type="text/javascript">
	   //点击弹出层城市选择
 	 	function region(){
			$("#layer").show();
			$("#layer").animate({top:'0'},300);
			}
		//点击关闭弹出层
		function down_none(){
			$("#layer").hide().animate({top:'800px'},300);
			}
		//县区弹出层
		function county(){
			$("#layer_1").slideDown(200);
 			}
		//县区弹出层关闭
		function county_no(){
			$("#layer_1").hide();
 			}
		//选择时间弹出层
		function time_box() {
			$('#time_box').slideDown();
		}
		//选择时间弹出层关闭
		function time_clos() {
			$('#time_box').slideUp();	
			$('#prompt_box').show();	
			$('.mask_layer').show();
		}
			//当前位置 弹出层
		function adders_show() {
			$('#addr_box').slideDown();
		
		}
		//当前位置关闭弹出层
		function adders_clos() {
			$('#addr_box').slideUp();	
			$('#time_box').slideDown();	
		}
		//关闭提示层
		function layer_clos() {
			$('#prompt_box').hide();	
			$('.mask_layer').hide();
		}
		// 切换地图、列表
		function tab(){
			if ( $('#flag').val() == 1 ){
				$('#flag').val('0');
				$('.list_wrap').show();
				$('.map_wrap').hide();
				$('#lisebox_').hide();
				$('#text1').html('切换地图')
			}else{
				$('#flag').val('1');
				$('.list_wrap').hide();
				$('.map_wrap').show();
				$('#lisebox_').show();
				$('#text1').html('切换列表')
			}
		}	
		document.getElementById('datePicker').valueAsDate = new Date();
    </script>
