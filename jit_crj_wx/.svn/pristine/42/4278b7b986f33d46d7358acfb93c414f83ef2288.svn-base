@charset "UTF-8";

/* CSS Document */

html { background:url(../image/images/bg.jpg) repeat; background-size:100% 100%; font-family:"微软雅黑，黑体"} 
#wrap { position:relative; max-width:640px; margin:0 auto;}
#header { width:100%; height:40px; line-height:40px; background-color:#0397f7; font-size:1.6em; color:#fff; text-align:center;}

/* 添加身份证信息 */
#section { width:90%; margin:0 auto; padding:50px 0 0 0; min-height:240px;}
@media screen and (min-width: 310px) and (max-width: 480px) {
    #section {
         min-height:100px;
    }
}
#section .id_main { width:100%; margin-bottom:}
#section .id_main dt { position:relative; padding-left:10px; font-size:1.2em; color:#767e89; line-height:40px;}
#section .id_main dd { width:100%;}
#section .id_main dd .input_sty {width:98%; padding:13px 0 13px 2%; border:1px solid #e0e0e0; border-radius:5px; background-color:#fff; color:#000;}
.border_color {border:1px solid #900!important;}
.delete_icon { position:absolute; right:0 ;top:0;}
.hit_show { position:fixed; bottom:65px; right:0; background-color:#fff; padding:10px;}
.hit_show p { line-height:25px; font-size:1em;}
.date_sty { width:100%; border:none; background-color:#fff; text-align:center; font-size:1.4em; color:#767e89;}

/*ios 取下默认按钮样式 */
input[type="button"], input[type="submit"], input[type="reset"],input[type="date"],input[type="input"],input[type="nume"]{
-webkit-appearance: none;
}

/* 添加协行人 */
.add_box { margin:10% 0 5% 0; padding:15px 0; width:99%; border:1px solid #e1e1e1; background-color:#e6e6e6; text-align:center;}
.add_box a { color:#a9a9a9; font-size:1.2em;}

/* 确定按钮 */
.btn_confirm { display:inline-block; margin:0 auto; width:45%; height:35px; line-height:35px; border-radius:30px; background-color:#26b2ed; color:#fff; font-size:1.2em; text-align:center;}
.btn_can { background-color:#CCC!important; margin-left:7%;}
.btn_width { width:80%!important;}

/* 地图预约 */
.list_text { float:right; padding-right:20px;font-size:0.8em!important; color:#fff;}
.nav_box { border-bottom:1px solid #e6e6e6; overflow:hidden; clear:both; background-color:#fff; } 
.nav_box li { display:inline-block;  width:30%;border-right:1px solid #e6e6e6; line-height:40px; text-align:center;}
.nav_box li:nth-child(2){ width:40%;}
.nav_box li:nth-child(3){ width:27%;}
.nav_box li a { width:80%; display:inline-block;color:#767e89; font-size:1.2em; background:url(../image/icon/up_icon.png) no-repeat right center;}
.nav_box li a.default { color:#0494f5; background:url(../image/icon/uo_icon_hove.png) no-repeat right center;}
.nav_box li:last-child { border-right:none;}
.hit { text-align:center; width:100%; line-height:30px; background-color:#f3f5f4; color:#f38004; font-size:1.2em;}

/* 切换列表 */
.lise_box { width:100%;}
.lise_box li { position:relative; padding:15px 20px 15px 35px; background:url(../image/icon/adders_01.png) no-repeat 10px 22px; background-size:12px 16px; font-size:1.2em; text-align:left; border-bottom:1px solid #d6d6d7; border-top:1px solid #fff; overflow:hidden;}
.lise_box li span { float:left; width:75%; line-height:27px; display:block;}
.time_btn {float:right; display:inline-block; width:60px; height:25px; line-height:25px; text-align:center; border:1px solid #7ed02e; border-radius:5px; color:#7ed02e;}

/* 选择城市弹出层 */
.city_box { width:100%; background-color:#f4f4f4; position:absolute; top:0px; left:0; z-index:5;}
.city_title_def { position:fixed; width:97%; padding-left:3%; height:35px; line-height:35px; color:#333; font-size:1.4em; background-color:#fff; font-weight:normal;}
.city_title { padding-left:10px; background-color:#ebebeb; height:25px; line-height:25px; color:#666; font-size:1.2em;}
.screening_box { padding-bottom:15px;}
.screening_box a.select_sty { padding:10px 1%; display:inline-block; margin:10px 0 0 3%; width:26%; color:#666; background:#fff; border:1px solid #d8d8d8; border-radius:3px; text-align:center; font-size:1.2em;}
.close_icon { position:absolute; top:0; left:46%; z-index:1;}
.close_icon_img { 
transform:rotate(180deg);
-ms-transform:rotate(180deg); /* IE 9 */
-webkit-transform:rotate(180deg); /* Safari and Chrome */
-o-transform:rotate(180deg);
-moz-transform:rotate(180deg);
}



/* 选择时间弹出层 */
.time_box { position:fixed; bottom:0; left:0;  width:100%; background-color:#fff; z-index:4;}
.time_main { margin:0 auto;  width:85%; border-left:2px solid #e1e1e1;}
.time_main_icon { position:absolute; top:0; left:-37px;}
.time_main li { position:relative; margin-left:20px; padding:10px; border-bottom:2px solid #e1e1e1;}
.time_main li:nth-of-type(1) {padding-top:40px;}
.time_main li:last-child {border-bottom:none;}
.time_main li p { line-height:30px; font-size:1.2em; text-align:left;} 
.text_color { color:#ffa300;}
.btn_appoin { position:absolute; top:30%; right:0; display:inline-block; width:70px; height:25px; line-height:25px; text-align:center; border:1px solid #0494f5; border-radius:40px; color:#0494f5; font-size:0.9em!important;}
.btn_right { right:10px!important;}
.btn_appoin_not { border:1px solid #d1d1d1!important;color:#d1d1d1;!important }

/* 预约信息 */
.succe_box { width:100%; border-radius:10px; background:#fff url(../image/background/secce_mian_bg.jpg) no-repeat right bottom; background-size:60px 93px;}
.succe_tit { height: 45px; line-height:45px;text-align:center; font-size:1.4em; color:#0395f8; background:#edf5ff url(../image/background/succes_tit_bg.jpg) no-repeat 20px bottom; border-radius:10px 10px 0 0 ;}
.succe_main { width:100%; margin:0 auto; }
.succe_main li { position:relative; padding-left:20px; border-bottom:1px solid #e5e5e5; color:#6c6c6c; font-size:1.2em; line-height:45px;}
.succe_main li:nth-child(2) { border-left:2px solid #039dff;}	
.succe_main li:nth-child(3) { border-left:2px solid #7ed02e;}
.succe_main li:nth-child(4) { border-left:2px solid #ffa300;}
.succe_main li:nth-child(5) { border-left:2px solid #039dff;}
.succe_main li p { padding-left:25px;}
.succe_main li p.li_p{ background:url(../image/background/time_icon1.png) no-repeat left center; background-size:18px 18px;}
.li_p1 { background:url(../image/background/adder_02.png) no-repeat left center;  background-size:14px 18px;}
.btn_cancel{ clear:right; position:absolute; top:60%;right:10px;}

/* 暂无数据提示 */
.no_data_box { position:relative; width:80%; margin:0 auto; background-color:#fff; border-radius:5px; padding:12% 5%;}
.no_data_box p { line-height:30px; font-size:1.4em;}
.no_data_icon { position:absolute; top:-40px; right:0;} 
.no_data_box p > a { color:#0791ea;}

/* 当前位置－列表 */
.addres_color { color:#a5a5a5!important;}
.distance_num { padding-left:15px; font-size:0.8em!important;}
.list_box_ { overflow-y:scroll; max-height:350px;}
.my_location {  padding:10px 5%; background:#fafafa; width:90%;}
.my_location p { line-height:30px; font-size:1.2em;}
.mask_layer { display:none; position:fixed; top:0; left:0;width:100%; height:100%; background:rgba(0,0,0,0.3); z-index:12}

/* 提示 */
.hit_main {position:absolute; top:40px; left:0; width:100%;}
.hit_main p { line-height:22px;!important;}

/*2016-5-6 */

/* 业务选择 */
.busi_box { margin:15% auto; width:85%; background:#ffffff; border-radius:8px; -webkit-border-radius:8px; -moz-border-radius:8px;}
.busi_tit { padding-left:5%; width:95%; height:35px; line-height:35px;background:#ededee; border-radius:8px 8px 0 0 ;
-webkit-border-radius:8px 8px 0 0 ; -moz-border-radius:8px 8px 0 0 ; color:#535252; font-size:1.2em; font-weight:normal; border-left:1px solid #4eb783}
.buse_ul { overflow:hidden; clear:both;}
.buse_ul li { position:relative; float:left; margin:35px 0; width:33%;text-align:center; cursor:pointer;}
.buse_ul li p { font-size:1.2em; padding-bottom:8px;}
.busi_tit_again{ border-radius:0;!important}
.span_dis { display:block; text-align:center; font-size:0.8em; color:#9f9e9e; min-height:15px;}
.padding_no li {vertical-align:top;}
.layer_selected { position:absolute; top:0; left:4%; width:92%; height:100%; background:rgba(0,0,0,0.2);}
.layer_selected img { margin-top:35%;}

/* 列表 */
.list_content_box { width:100%;}
/* @media (min-width: 360px) {
    .list_content_box { height:500px;}
}
@media (min-width:768px) {
    .list_content_box { height:900px;}
}
 */
.list_main { position:relative; margin:15px auto; width:92%; border-radius:8px; background-color:#fff; box-shadow:1px 1px 1px #d5d5d5;}
.list_main .list_dl_box { position:relative; overflow:hidden; clear:both; padding:15px; background-color:#ededee; border-bottom:1px solid #dcdcdc; border-radius:8px 8px 0 0;}
.list_main .list_dl_box dt { float:left; width:18%; text-align:center;}
.list_main .list_dl_box dd { float:left; width:82%;}
.list_main .list_dl_box dd p{ line-height:25px; font-size:1.2em;} 
.list_ul_box { overflow:hidden; clear:both; padding:0 17px 17px 17px;}
.list_ul_box li { float:left; padding-top:17px; width:25%; text-align:center; cursor:pointer;}

/* 判断最小屏幕300 最大360时执行代码 */
@media (min-width: 300px) and (max-width: 360px) {
    .list_ul_box li { width:33%; }
}
.list_ul_box li p { line-height:20px; height:20px; font-size:1.2em}
.list_ul_box li p:nth-child(1) { margin:0 auto; width:25px; height:25px; line-height:25px!important; border-radius:50%; border:1px solid #0397f7; color:#0397f7;}
.list_ul_box li p:nth-child(2) { color:#ffa300!important;}
.list_ul_box li p:nth-child(3) { font-size:1em; color:#a8a8a8;}
.icon_right { position:absolute; top:0; right:3%;}
.text_position { position:absolute; right:3%; bottom:18px;}



