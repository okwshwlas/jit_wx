body, h1, div, li, ul, img, span, p ,h1,h2,h3,h4,h5,h6,form,dl,dt,dd,input{
	margin: 0;
	padding: 0;
}
html {
	position: static;
}
li {
	list-style-type: none;
}
img {
	border: 0;
}
body {
	font-family: Verdana,宋体;
	font-size: 12px;

}
h1, h2, h3, h4, h5, h6 {
	font-size: 12px;
}



.step-panel {
	float: left;
	width: 150px;
	height: 100%;
	background:#f1fcff;
	border: 1px solid #ccc;
}
.step-panel-content {
	float: left;
}
.step-panel li {
	height: 36px;
	width: 87px;
	margin: 5px auto;
	color: #0fb9e1;
	font-weight: bold;
	text-align: center;
	padding-top: 58px;
	display: block;
	cursor: hand;
}
.step-panel li.status-1 {
	background:url(../images/step-bg.gif) no-repeat;
}
.step-panel li.status-2 {
	background:url(../images/step-bg.gif) -183px 0  no-repeat;
}
.step-panel li.status-3 {
	background:url(../images/step-bg.gif) -91px 0 no-repeat;
	color:#999;
}
.tips-panel {
	font-size: 14px;
	font-weight: bold;
	margin: 20px auto 10px 30px;
	line-height: 60px;
}
.tips-panel .icon {
	padding-top: 10px;
	margin-right: 20px;
	float: left;
}

/*============浮动快速功能 start=============*/
#floatMenu dl,dt,dd,h5,p{
	padding:0px;
	margin:0px;
	font-size:10pt;}
#floatMenu dl{
	width:115px;
	height:auto;
	display:block;
	overflow:hidden;
	margin:0 auto;
	font-size:10pt;
	line-height:160%;}
#floatMenu dt,dt a{
	width:100%;
	height:auto;
	display:inline;
	font-weight: bold;
	color:#333;
	font-size:10.5pt;
	cursor:pointer;
	text-decoration: none;
}
#floatMenu dt a:hover{
	text-decoration: underline;
	color: #FF0000;
}

#floatMenu 
{
	padding-top: 0px;
	border: 1px solid #dcdcdc;
	position: absolute;
	float: right;
	top: 100px;
	right: 5px;
	width: 115px;
	z-index: 1;
} 
.float-qucik {
	background:#FFF;
	width: 113px;
}
.float-qucik td {
	font-weight: bold;
	color: #000;
}
.float-qucik-content {
	background:url(../images/float-quick-bg.gif) bottom repeat-x;
}
.float-brand {
	text-align: center;
	margin: 5px auto 5px auto;
}
.float-list {
	margin-top: 0px;
}
.float-list li {
	background: url(../images/flpat-list-bg.gif) no-repeat;
	line-height: 24px;
	height: 24px;
	text-indent: 30px;
	color: #2271ca;
}
.float-list li a:link,.float-list li a:visited,.float-list li a:hover {
	color: #2271ca;
	text-decoration: none;
}
/*============浮动快速功能 end=============*/