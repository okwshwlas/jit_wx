.table-input {
	font-size: 12px;
	margin: 0 auto;
	margin-bottom:10px;
	width:98%;
	border-left:1px #C1DAD7 solid;
	border-top:1px #C1DAD7 solid;
}
.table-input td {
	border-right: 1px solid #C1DAD7;
	border-bottom: 1px solid #C1DAD7;
	background: #fff;
	font-size: 12px;
	padding: 3px 6px 3px 12px;
	color: #4f6b72;
	white-space: nowrap;
}

.table-input th {
	color: #4f6b72;
	border-right: 1px solid #C1DAD7;
	border-bottom: 1px solid #C1DAD7;

	letter-spacing: 2px;
	text-transform: uppercase;
	text-align: left;
	padding: 3px 6px 3px 12px;
	white-space: nowrap;
}
.table-input th.specalt {
	border-top: 0;
	color: #000000;
	text-align:right;

}