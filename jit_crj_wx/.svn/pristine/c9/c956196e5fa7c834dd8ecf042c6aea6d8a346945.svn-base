a {
	text-decoration: none;
}
.checkbox {
	padding: 0;
	margin: 0;
	height: 14px;
}
.btn {
	padding: 2px 4px;
	border: 1px solid #73B2DE;
	background: #ADD9FE url('../images/button.jpg') repeat-x;
	color: #4F60BD;
	margin: 2px;
	display: block;
	float: left;
}
.btn.right {
	float: right;
}
.btn.return {
	background: #ADD9FE url('../images/button.jpg') repeat-x;
}
.btn.nofloat{
	display:inline;
	float:none;
}

input.btn{
	padding: 0 4px;
	border: 1px solid #73B2DE;
	background: #ADD9FE url('../images/button.jpg') repeat-x;
	color: #4F60BD;
	margin: 0;
	display:inline-block;
	float:none;
	height:20px;
	margin-left:4px;
	line-height:20px;

}

.tooltip {
	position: absolute;
	top: 0;
	left: 0;
	z-index: 3;
	display: none;
	background:#FFFFCC;
	border:1px gray solid;
	padding:6px;
	width:300px;
	white-space:normal;
}
.checkTip{
	position: absolute;
	margin:2px 0 0 2px;
	z-index;10px;
	border:1px solid #000;
	padding:2px 6px;
	background:#333;
	color:#fff;
	cursor:pointer;
}
